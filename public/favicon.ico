<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100">
  <!-- Fleet truck icon in orange/blue -->
  <rect width="100" height="100" fill="#2a2a2a"/>
  
  <!-- Truck body -->
  <rect x="20" y="40" width="50" height="30" fill="#ff9500" stroke="#ff8c00" stroke-width="2"/>
  
  <!-- Truck cab -->
  <rect x="65" y="30" width="20" height="40" fill="#ff9500" stroke="#ff8c00" stroke-width="2"/>
  
  <!-- Window -->
  <rect x="68" y="35" width="14" height="12" fill="#add8e6" stroke="#6495ed" stroke-width="1"/>
  
  <!-- Wheels -->
  <circle cx="32" cy="70" r="8" fill="#333" stroke="#111" stroke-width="2"/>
  <circle cx="32" cy="70" r="3" fill="#ccc"/>
  
  <circle cx="63" cy="70" r="8" fill="#333" stroke="#111" stroke-width="2"/>
  <circle cx="63" cy="70" r="3" fill="#ccc"/>
  
  <!-- TT text -->
  <text x="30" y="28" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#ff9500">TT</text>
</svg>
